<h2>Manufacturer</h2>

<dl>
<dt>Name:</dt>
<dd> <%= @manufacturer.manufacturer_name() %></dd>
<dt>Website:</dt>
<dd> <a href="<%=@manufacturer.website()%>"><%= @manufacturer.website() %> </a></dd>
<dt>Contact Name:</dt>
<dd><%= @manufacturer.contact_name() %></dd>
<dt>Email:</dt>
  <dd> <%= @manufacturer.email() %></dd>
<dt>Phone:</dt>
<dd> <%= @manufacturer.phone_number() %></dd>
<dt>Notes:</dt>
<dd> <%= @manufacturer.notes() %></dd>
</dl>

<form action="/inventory/manufacturer/<%= @manufacturer.id %>/edit" method="get">
  <input type="submit" value="Update Manufacturer">
</form>

<form action="/inventory/manufacturer/<%= @manufacturer.id %>/delete" method="post">
  <input type="submit" value="Delete Manufacturer">
</form>
